{
  "name": "dword",
  "version": "7.0.8",
  "description": "Web editor based on CodeMirror",
  "main": "server/index.js",
  "bin": {
    "dword": "bin/dword.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "bin/dword.js package.json",
    "start:dev": "NODE_ENV=development npm start",
    "lint": "redrun lint:*",
    "lint:bin": "eslint --rule 'no-console:0' bin",
    "lint:client": "eslint --env browser --rule 'no-console:0' client",
    "lint:server": "eslint server",
    "build-progress": "webpack --progress",
    "build:client": "npm run build-progress -- --mode production",
    "build:client:dev": "NODE_ENV=development npm run build-progress -- --mode development",
    "cp:codemirror:modules": "cp -r node_modules/codemirror modules/",
    "build-progress:codemirror": "webpack --progress --config ./webpack.config.codemirror.js",
    "build:codemirror:dev": "NODE_ENV=development npm run build-progress:codemirror --mode development",
    "build:codemirror": "npm run build-progress:codemirror --mode production",
    "watch:server": "npm run watch \"npm start\"",
    "watch:client": "npm run watch \"npm run build:client:dev\"",
    "watch": "nodemon -w server -w client -x",
    "build": "redrun build:client* build:codemirror* cp:codemirror:*",
    "wisdom": "npm run build",
    "rm:dist": "rimraf dist",
    "rm:dist-dev": "rimraf dist-dev"
  },
  "dependencies": {
    "ashify": "^1.0.1",
    "currify": "^2.0.0",
    "express": "^4.14.0",
    "fullstore": "^1.0.0",
    "join-io": "^2.0.0",
    "patchfile": "^1.0.1",
    "readjson": "^1.1.0",
    "restafary": "^3.0.0",
    "socket-file": "^2.0.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "@cloudcmd/clipboard": "^1.0.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "clean-css-loader": "^1.0.1",
    "codemirror": "^5.38.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.7.7",
    "daffy": "^1.0.3",
    "eslint": "^4.0.0",
    "execon": "^1.2.9",
    "load.js": "^1.1.4",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.2",
    "smalltalk": "^3.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "wraptile": "^2.0.0",
    "zipio": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudcmd/dword"
  },
  "keywords": [
    "editor",
    "express",
    "middleware",
    "monaco",
    "patch",
    "diff",
    "ide",
    "text"
  ],
  "author": "coderaiser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudcmd/dword/issues"
  },
  "homepage": "https://github.com/cloudcmd/dword"
}
